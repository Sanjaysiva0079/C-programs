#include<stdio.h>
int main()
{
    
    int arr[100]={1,2,3,4,2,5,5,7};
    int n=8;
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            
            
                if (arr[i]==arr[j])
                {
                    for(int k=j;k<n-1;k++)
                    {
                        arr[k]=arr[k+1];
                    }
                    n--;
                    j--;
                }
            
        }
        
    }
        for(int i=0;i<n;i++)
        {
            printf("%d",arr[i]);
        }
    
    
    
    return 0;
    
    
}
