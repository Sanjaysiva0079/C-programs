#include <stdio.h>
#include <ctype.h>
#include<string.h>

int isvowel(char ch){
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'){
        return 1;
    }
    return 0;
}
int main()
{ char a[20];
  scanf("%s",a); int n=strlen(a);
  char res[20];
  int k=0;
          for(int i=0;i<n;i++){
              char ch=tolower(a[i]);
              if(isvowel(ch)){
                  res[k++]=ch;}
              }
              for(int i=0;i<n;i++){
              char ch=tolower(a[i]);
              if(!isvowel(ch)){
                  res[k++]=ch;}
              }
              res[k]='\0';
              printf("%s",res);
              return 0;
}
