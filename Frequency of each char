#include <stdio.h>

int main()
{
   char str[20];
   scanf("%s",&str);
    for(int i=0;str[i]!='\0';i++)
    {
     int count=0;
     int ac=0;
     for(int k=0;k<i;k++)
     {
         if(str[k]==str[i])
         {
             ac=1;
             break;
         }
     }
     if(ac)
     {
         continue;
     }
    for(int j=0;str[j]!='\0';j++)
     {
         if(str[i]==str[j])
         {
             count++;
         }
     }
     printf("%c - %d ",str[i],count);
     
    }
     return 0;
     
    }
