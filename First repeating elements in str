#include <stdio.h>
int main()
{
    char str[20];
    scanf("%s",&str);
    for(int i=0;str[i]!='\0';i++)
    {
        int count=0;
        for(int j=i+1;str[j]!='\0';j++)
        {
            if(str[i]==str[j])
            {
                count++;
                
            }
            if(count==1)
            {
                printf("%c",str[i]);
                return 0;
            }

        }
    }
 
    return 0;
}
