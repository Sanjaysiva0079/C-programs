#include <stdio.h>
#include <stdlib.h>

struct node {
    char data;
    struct node *next;
} *top = NULL;


void push(char data)
{
    struct node *newnode = (struct node*)malloc(sizeof(struct node));
    newnode->data = data;
    newnode->next = top;
    top = newnode;
}


char pop()
{
    if (top == NULL)
        return '#';  

    struct node *temp = top;
    char val = temp->data;
    top = temp->next;
    free(temp);
    return val;
}


int match(char open, char close)
{
    if (open == '(' && close == ')') return 1;
    if (open == '{' && close == '}') return 1;
    if (open == '[' && close == ']') return 1;
    return 0;
}


int isBalanced(char *str)
{
    for (int i = 0; str[i] != '\0'; i++)
    {
        if (str[i] == '(' || str[i] == '{' || str[i] == '[')
        {
            push(str[i]);
        }
        else
        {
            char open = pop();
            if (!match(open, str[i]))
                return 0;
        }
    }

    return (top == NULL); 
}

int main()
{
    int n;
    scanf("%d", &n);

    while (n--)
    {
        char str[1000];
        top = NULL;  
        scanf("%s", str);

        if (isBalanced(str))
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}
